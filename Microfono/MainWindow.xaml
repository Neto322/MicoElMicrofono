        <Button Content="STAPH!!" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="75" Background="#FF1ADC7B" Foreground="#FFFF00DC" Click="Button_Click"/>
        <TextBlock x:Name="LblHertz" HorizontalAlignment="Left" Margin="299,43,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="0.546,0.102" Height="27" Width="83"/>

    </Grid>
